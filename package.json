{
  "name": "editdata.org",
  "description": "edit tabular data. save it on github.",
  "version": "1.0.0",
  "repository": {
    "url": "git://github.com/flatsheet/editdata.org.git"
  },
  "main": "app.js",
  "scripts": {
    "deploy": "npm run bundle && surge . editdata.org",
    "bundle-css": "cssnext assets/deps.css assets/bundle.css",
    "bundle-js": "browserify app.js -t [ envify --NODE_ENV production ] -o  assets/bundle.js",
    "bundle": "npm run bundle-css && npm run bundle-js",
    "start": "budo app.js:assets/bundle.js --live --pushstate -- -t [ envify --NODE_ENV development ] -t brfs"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "dependencies": {
    "JSONStream": "^1.0.4",
    "base-element": "^3.0.1",
    "basscss-grid": "^1.0.3",
    "brfs": "^1.4.1",
    "browser-fs": "0.0.4",
    "cookie-cutter": "^0.1.1",
    "csskit": "^1.1.1",
    "csv-parser": "^1.7.0",
    "csv-write-stream": "^1.0.0",
    "cuid": "^1.3.8",
    "dat-core": "^5.0.1",
    "data-schema": "^1.4.3",
    "data-set": "^4.0.0",
    "detect-json-style": "^1.0.2",
    "element-class": "^0.2.1",
    "envify": "^3.4.0",
    "extend": "^3.0.0",
    "file-extension": "^1.1.4",
    "filter-object": "^2.0.0",
    "from2": "^2.1.0",
    "from2-array": "0.0.4",
    "from2-string": "^1.1.0",
    "github-api": "^0.10.6",
    "hash-match": "^1.0.1",
    "history-state": "^1.0.0",
    "inherits": "^2.0.1",
    "keycode": "^2.1.0",
    "keymaster": "^1.6.2",
    "level-js": "^2.2.1",
    "levelup": "^1.2.1",
    "lodash.debounce": "^3.1.1",
    "lodash.union": "^3.1.0",
    "menu-element": "^2.0.0",
    "mime-types": "^2.1.5",
    "normalize.css": "^3.0.3",
    "parse-link-header": "^0.4.1",
    "pathname-match": "^1.1.0",
    "popupjs": "^1.3.0",
    "simple-local-storage": "^1.0.3",
    "through2": "^2.0.0",
    "uglifyify": "^3.0.1",
    "view-list": "shama/view-list",
    "virtual-dom": "^2.1.1",
    "wayfarer": "^4.2.0",
    "xhr": "^2.0.3"
  },
  "devDependencies": {
    "brfs": "^1.4.1",
    "browserify": "^11.0.1",
    "budo": "^4.1.0",
    "cssnext": "^1.8.2",
    "dom-value": "0.0.0",
    "sheetify": "^2.0.0",
    "surge": "^0.14.3"
  }
}
